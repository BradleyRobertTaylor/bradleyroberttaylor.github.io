---
import BaseLayout from '../layouts/BaseLayout.astro';
---

<BaseLayout>
  <div class="flex justify-center items-center min-h-screen">
    <main class="text-center">
      <div class="space-y-2">
        <h1 class="text-4xl">404</h1>
        <p>This page doesn't exist</p>
        <a
          href="/"
          class="text-medium text-neutral-400 hover:text-neutral-700 dark:text-neutral-400 dark:hover:text-inherit transition-colors"
          >Go back home</a
        >
      </div>
    </main>
  </div>
</BaseLayout>
